{
  "name": "@billsdk/stripe",
  "version": "0.1.4",
  "description": "Stripe payment adapter for BillSDK",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "decker",
    "url": "https://github.com/decker-dev"
  },
  "homepage": "https://github.com/commet-labs/billsdk/tree/main/packages/stripe#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commet-labs/billsdk.git",
    "directory": "packages/stripe"
  },
  "bugs": {
    "url": "https://github.com/commet-labs/billsdk/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/commet-labs"
  },
  "engines": {
    "node": ">=20"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "clean": "rm -rf dist .turbo .node_modules"
  },
  "peerDependencies": {
    "@billsdk/core": "workspace:*",
    "stripe": ">=14.0.0"
  },
  "devDependencies": {
    "@billsdk/core": "workspace:*",
    "@biomejs/biome": "2.3.11",
    "@types/node": "22.15.29",
    "stripe": "14.0.0",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.17"
  },
  "keywords": [
    "billing",
    "sdk",
    "stripe",
    "payments",
    "adapter",
    "subscriptions",
    "checkout",
    "metered-billing"
  ]
}
