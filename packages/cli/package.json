{
  "name": "@billsdk/cli",
  "version": "0.1.0",
  "description": "CLI for BillSDK - Generate schemas and manage billing configuration",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "decker",
    "url": "https://github.com/decker-dev"
  },
  "homepage": "https://billsdk.com/docs/concepts/cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commet-labs/billsdk.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/commet-labs/billsdk/issues"
  },
  "engines": {
    "node": ">=20"
  },
  "bin": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "clean": "rm -rf dist .turbo node_modules"
  },
  "dependencies": {
    "@billsdk/core": "workspace:*",
    "@clack/prompts": "1.0.0",
    "c12": "3.3.3",
    "chalk": "5.6.2",
    "commander": "14.0.3",
    "dotenv": "17.2.3",
    "prettier": "3.8.1"
  },
  "devDependencies": {
    "@babel/core": "7.29.0",
    "@babel/preset-react": "7.28.5",
    "@babel/preset-typescript": "7.28.5",
    "@biomejs/biome": "2.3.11",
    "@types/node": "22.15.29",
    "billsdk": "workspace:*",
    "drizzle-orm": "0.45.1",
    "jiti": "2.6.1",
    "tsup": "8.5.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.17"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "billing",
    "sdk",
    "cli",
    "schema",
    "generator",
    "drizzle",
    "prisma"
  ]
}
